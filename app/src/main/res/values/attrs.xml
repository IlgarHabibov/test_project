<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="IndicatorView">
        <attr name="activeDrawable" format="reference"/>
        <attr name="deActiveDrawable" format="reference"/>
        <attr name="childCount" format="integer"/>
        <attr name="currentStep" format="integer"/>
        <attr name="itemSize" format="dimension"/>
        <attr name="itemPadding" format="dimension"/>
